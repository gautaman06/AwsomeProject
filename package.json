{
  "name": "project-y",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start-local-server": "npx expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "format": "prettier --write ./** ",
    "lint": "eslint .",
    "lint-fix": "eslint .",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "build:dev:android": "eas build --profile development --platform android",
    "build:prod:android": "eas build --platform android",
    "build": "eas build --platform all",
    "eject": "react-scripts eject",
    "husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "tunnel": "npx expo start --tunnel"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "svgo": "^2.8.0",
    "@svgr/babel-preset": "^6.1.0",
    "@svgr/plugin-jsx": "^6.1.2",
    "@svgr/plugin-svgo": "^6.1.2"
  },
